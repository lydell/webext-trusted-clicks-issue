{
  "manifest_version": 2,
  "name": "webext-trusted-clicks-issue",
  "version": "0.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "match_about_blank": true,
      "js": [
        "content.js"
      ]
    }
  ]
}
